<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="bpim-adminrole" namespace="/employeeManage" extends="bpim-default">
		<action name="saveRole" method="save" class="adminRoleAction">
			<result type="redirect-action">listRole.do</result>
		</action>
		<action name="listRole" method="list" class="adminRoleAction">
			<result>/employeeManage/queryRole.jsp</result>
		</action>
		<action name="getRole" method="get" class="adminRoleAction">
			<result>/employeeManage/updateRole.jsp</result>
		</action>
		<action name="updateRole" method="update" class="adminRoleAction">
			<result type="redirect-action">listRole.do</result>
		</action>
		<action name="deleteRole" method="delete" class="adminRoleAction">
			<result type="redirect-action">listRole.do</result>
		</action>
	</package>
</struts>