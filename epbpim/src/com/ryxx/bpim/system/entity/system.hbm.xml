<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.ryxx.bpim.system.entity">
	<class name="NewsAnnouncement" table="NEWS_ANNOUNCEMENT">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="identity"></generator>
		</id>
		<property name="newsTitle" column="NEWS_TITLE" type="java.lang.String"></property>
		<property name="content" column="NEWS_CONTENT" type="java.lang.String"></property>
		<property name="addTime" column="ADD_TIME" type="java.sql.Timestamp"></property>
	</class>
</hibernate-mapping>