<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="com.ryxx.bpim.dao.AdminMenuDAO" class="com.ryxx.bpim.dao.AdminMenuDAOImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	
	<bean id="com.ryxx.bpim.service.AdminMenuService.target" class="com.ryxx.bpim.service.AdminMenuServiceImpl">
	</bean>
	<bean id="com.ryxx.bpim.service.AdminMenuService" parent="baseTransactionProxyCdm">
		<property name="target" ref="com.ryxx.bpim.service.AdminMenuService.target" />
	</bean>
	
	<bean id="com.ryxx.bpim.dao.AdminLogDAO" class="com.ryxx.bpim.dao.AdminLogDAOImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	<bean id="com.ryxx.bpim.service.AdminLogService.target" class="com.ryxx.bpim.service.AdminLogServiceImpl">
	</bean>
	<bean id="com.ryxx.bpim.service.AdminLogService" parent="baseTransactionProxyCdm">
		<property name="target" ref="com.ryxx.bpim.service.AdminLogService.target" />
	</bean>
	
	<bean id="com.ryxx.bpim.dao.AdminUserDAO" class="com.ryxx.bpim.dao.AdminUserDAOImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	<bean id="com.ryxx.bpim.service.AdminUserService.target" class="com.ryxx.bpim.service.AdminUserServiceImpl">
	</bean>
	<bean id="com.ryxx.bpim.service.AdminUserService" parent="baseTransactionProxyCdm">
		<property name="target" ref="com.ryxx.bpim.service.AdminUserService.target" />
	</bean>
	<bean id="adminUserAction" class="com.ryxx.bpim.action.AdminUserAction" scope="prototype">
		<property name="adminUserService">
			<ref local="com.ryxx.bpim.service.AdminUserService"/>
		</property>
	</bean>
	
	<bean id="com.ryxx.bpim.dao.ProvinceCityDAO" class="com.ryxx.bpim.dao.ProvinceCityDAOImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	<bean id="com.ryxx.bpim.service.ProvinceCityService.target" class="com.ryxx.bpim.service.ProvinceCityServiceImpl">
	</bean>
	<bean id="com.ryxx.bpim.service.ProvinceCityService" parent="baseTransactionProxyCdm">
		<property name="target" ref="com.ryxx.bpim.service.ProvinceCityService.target" />
	</bean>

	<bean id="com.ryxx.bpim.dao.EnterpriseInfoDAO" class="com.ryxx.bpim.dao.EnterpriseInfoDAOImpl">
		<property name="sessionFactory" ref="sessionFactory"></property>
	</bean>
	<bean id="com.ryxx.bpim.service.EnterpriseInfoService.target" class="com.ryxx.bpim.service.EnterpriseInfoServiceImpl">
	</bean>
	<bean id="com.ryxx.bpim.service.EnterpriseInfoService" parent="baseTransactionProxyCdm">
		<property name="target" ref="com.ryxx.bpim.service.EnterpriseInfoService.target" />
	</bean>
	<bean id="enterpriseInfoAction" class="com.ryxx.bpim.web.action.EnterpriseInfoAction" scope="prototype">
		<property name="enterpriseInfoService">
			<ref local="com.ryxx.bpim.service.EnterpriseInfoService"/>
		</property>
		<property name="provinceCityService">
			<ref local="com.ryxx.bpim.service.ProvinceCityService"/>
		</property>
	</bean>
</beans>