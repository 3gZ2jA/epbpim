<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="bpim-login" namespace="/login" extends="bpim-default">
		<action name="index">
			<result>/jsp/login/index.jsp</result>
		</action>
		<action name="logout" class="logoutAction">
			<result>/jsp/login/index.jsp</result>
		</action>
		<action name="login" method="login" class="userInfoMainAction" >
			<result name="input">/jsp/login/index.jsp</result>
			<result name="enterpriseMain" type="redirect-action">../main/enterpriseMain.do</result>
			<result name="userMain" type="redirect-action"> ../main/main.do</result>
			<result name="userInfoManage">/jsp/login/updateUserInfo.jsp</result>
		</action>
		<action name="fillUserInfo" method="updateUserInfo" class="userInfoMainAction" >
			<result type="redirect-action"> ../main/main.do</result>
		</action>
		<action name="findPassword" >
			<result>/jsp/login/findPassword.jsp</result>
		</action>
		<action name="contactUs">
			<result>/jsp/login/contactUs.jsp</result>
		</action>
		<action name="sendPassword" method="sendPasswordByEmail" class="userInfoMainAction" >
			<result name="input">/jsp/login/findPassword.jsp</result>
		</action>
	</package>
	
	<package name="bpim-userinfo" namespace="/admin" extends="bpim-default">
		<action name="listUser" method="list" class="userInfoAction">
			<result>/jsp/admin/listUser.jsp</result>
		</action>
		<action name="newUser" method="newUser" class="userInfoAction">
			<result>/jsp/admin/addUser.jsp</result>
		</action>
		<action name="batchUser" method="batch" class="userInfoAction">
			<result type="redirect">listUser.do?eId=${eId}</result>
		</action>
		<action name="showUser" method="get" class="userInfoAction">
			<result>/jsp/admin/updateUser.jsp</result>
		</action>
		<action name="saveUser" method="save" class="userInfoAction">
			<result type="redirect">listUser.do?eId=${eId}</result>
		</action>
		<action name="updateLock" method="updateLock" class="userInfoAction">
			<result type="redirect">listUser.do?eId=${eId}</result>
		</action>
	</package>
	<package name="bpim-adminuser" namespace="/admin" extends="bpim-default">
	    <action name="login" method="login" class="adminUserAction">
	    	<result name="input">/jsp/admin/adminLogin.jsp</result>
			<result type="redirect-action">listEnterpriseInfo.do</result>
		</action>
		<action name="listNews" method="listNewsAnnounces" class="newAnnounceAction">
			<result>/jsp/admin/listNews.jsp</result>
			<interceptor-ref name="manager"/>
		</action>
		<action name="addNews" method="insertNewsAnnounces" class="newAnnounceAction">
			<result type="redirect-action">listNews.do</result>
			<interceptor-ref name="manager"/>
		</action>
		<action name="showNews" method="getDetailNewsAnnounces" class="newAnnounceAction">
			<result>/jsp/admin/updateNews.jsp</result>
			<interceptor-ref name="manager"/>
		</action>						  
		<action name="updateNews" method="updateNewsAnnounces" class="newAnnounceAction">
			<result type="redirect-action">listNews.do</result>
			<interceptor-ref name="manager"/>
		</action>
		<action name="deleteNews" method="deleteNewsAnnounces" class="newAnnounceAction">
			<result type="redirect-action">listNews.do</result>
			<interceptor-ref name="manager"/>
		</action>
		<action name="listUserAdvice" method="searchUserAdvice" class="userAdviceAction">
			<result>/jsp/admin/listUserAdvice.jsp</result>
			<interceptor-ref name="manager"/>
		</action>		
		<action name="importGovernmentFile" method="importGovernmentFile" class="governmentFileAction">
			<result type="redirect">preImportGovernmentFile.do?msg=${msg}</result>
			<interceptor-ref name="manager"/>
		</action>
		<action name="importGuidePrice" method="importGuidePrice" class="guidePriceAction">
			<result type="redirect">preImportGuidePrice.do?msg=${msg}</result>
			<interceptor-ref name="manager"/>
		</action>
		<action name="deleteGuidePrice" method="deleteGuidePrice" class="guidePriceAction">
			<result type="redirect">preDeleteGuidePrice.do?msg=${msg}</result>
			<interceptor-ref name="manager"/>
		</action>
		<action name="importGuideData" method="importGuideData" class="guideDataAction">
			<result type="redirect">preImportGuideData.do?msg=${msg}</result>
			<interceptor-ref name="manager"/>
		</action>
		<action name="deleteGuideData" method="deleteGuideData" class="guideDataAction">
			<result type="redirect">preDeleteGuideData.do?msg=${msg}</result>
			<interceptor-ref name="manager"/>
		</action>
		<action name="importPeriodicalFile" method="importPeriodicalFile" class="periodicalFileAction">
			<result type="redirect">preImportPeriodicalFile.do?msg=${msg}</result>
			<interceptor-ref name="manager"/>
		</action>
		<action name="uploadPricePackageFile" method="addPricePackageFile" class="pricePackageFileAction">
			<result type="redirect">preUploadPricePackageFile.do?msg=${msg}</result>
			<interceptor-ref name="manager"/>
		</action>
		<action name="preAddNews">
			<result>/jsp/admin/addNews.jsp</result>
		</action>
		<action name="preImportGovernmentFile" method="listProvinceAndMajor" class="listProvinceAndMajorAction">
			<result>/jsp/admin/importGovernmentFile.jsp</result>
		</action>
		<action name="preImportGuidePrice" method="listProvinceAndMajor" class="listProvinceAndMajorAction">
			<result>/jsp/admin/importGuidePrice.jsp</result>
		</action>
		<action name="preDeleteGuidePrice" method="listProvinceAndMajor" class="listProvinceAndMajorAction">
			<result>/jsp/admin/delGuidePrice.jsp</result>
		</action>
		<action name="preImportGuideData" method="listProvinceAndMajor" class="listProvinceAndMajorAction">
			<result>/jsp/admin/importGuideData.jsp</result>
		</action>
		<action name="preDeleteGuideData" method="listProvinceAndMajor" class="listProvinceAndMajorAction">
			<result>/jsp/admin/delGuideData.jsp</result>
		</action>
		<action name="preImportPeriodicalFile" method="listProvinceAndMajor" class="listProvinceAndMajorAction">
			<result>/jsp/admin/importPeriodicalFile.jsp</result>
		</action>
		<action name="preUploadPricePackageFile" method="listProvinceAndMajor" class="listProvinceAndMajorAction">
			<result>/jsp/admin/uploadPricePackageFile.jsp</result>
		</action>	
	</package>
	<package name="bpim-enterpriseinfo" namespace="/admin" extends="bpim-default">
		<action name="listEnterpriseInfo" method="list" class="enterpriseInfoAction">
			<result>/jsp/admin/listCompany.jsp</result>
		</action>
		<action name="newEnterpriseInfo" method="newEnterpriseInfo" class="enterpriseInfoAction">
			<result>/jsp/admin/addCompany.jsp</result>
		</action>
		<action name="addEnterpriseInfo" method="add" class="enterpriseInfoAction">
			<result type="redirect-action">listEnterpriseInfo.do</result>
		</action>
		<action name="showEnterpriseInfo" method="get" class="enterpriseInfoAction">
			<result>/jsp/admin/updateCompany.jsp</result>
		</action>
		<action name="updateEnterpriseInfo" method="update" class="enterpriseInfoAction">
			<result type="redirect-action">listEnterpriseInfo.do</result>
		</action>
		<action name="deleteEnterpriseInfo" method="delete" class="enterpriseInfoAction">
			<result type="redirect-action">listEnterpriseInfo.do</result>
		</action>
		<action name="newEnterpriseInfo" method="newEnterpriseInfo" class="enterpriseInfoAction">
			<result>/jsp/admin/addCompany.jsp</result>
		</action>
		<action name="balanceEnterpriseInfo" method="balance" class="enterpriseInfoAction">
			<result type="redirect-action">listEnterpriseInfo.do</result>
		</action>
		<action name="listRecharge" method="listRecharge" class="enterpriseInfoAction">
			<result>/jsp/admin/listRecharge.jsp</result>
		</action>
	</package>
	<package name="bpim-searchGovernmentInventory" namespace="/main" extends="bpim-default">
		<action name="searchGovernmentInventory" method="searchGovernmentInventory" class="searchGovernmentInventoryAction">
			<result name="inventory2003">/jsp/main/simple/listGovernmentInventory_2003.jsp</result>
			<result name="inventory2008">/jsp/main/simple/listGovernmentInventory_2008.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
	</package>
	<package name="bpim-searchGovernmentFile" namespace="/main" extends="bpim-default">
		<action name="searchGovernmentFile" method="searchGovernmentFile" class="searchGovernmentFileAction">
			<result>/jsp/main/simple/listGovernmentFile.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
	</package>
	<package name="bpim-governmentQuota" namespace="/main"	extends="bpim-default">
		<action name="searchGovernmentQuota" method="searchGovernmentQuota"
			class="searchGovernmentQuotaAction">
			<result>/jsp/main/simple/searchGovernmentQuota.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
		<action name="viewGovernmentQuotaDetail" method="listGovernmentQuotaDetail"
			class="governmentQuotaDetailAction">
			<result>/jsp/main/simple/viewGovernmentQuotaDetail.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
	</package>
	<package name="bpim-viewGovernmentFile" namespace="/main" extends="bpim-default">
		<action name="viewGovernmentFile" method="viewGovernmentFile" class="searchGovernmentFileAction">
			<result>/jsp/main/simple/showGovernmentFileDetail.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
	</package>
	<package name="bpim-searchGuidePrice" namespace="/main" extends="bpim-default">
		<action name="searchGuidePrice" method="searchGuidePrice" class="guidePriceAction">
			<result>/jsp/main/simple/listGuidePrice.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
	</package>
	<package name="bpim-searchGuideData" namespace="/main" extends="bpim-default">
		<action name="searchGuideData" method="searchGuideData" class="guideDataAction">
			<result>/jsp/main/simple/listGuideData.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
	</package>
	<package name="bpim-searchPeriodicalFile" namespace="/main" extends="bpim-default">
		<action name="searchPeriodicalFile" method="searchPeriodicalFile" class="periodicalFileAction">
			<result>/jsp/main/simple/listPeriodicalFile.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
	</package>
	<package name="bpim-viewPeriodicalFile" namespace="/main" extends="bpim-default">
		<action name="viewPeriodicalFile" method="viewPeriodicalFile" class="periodicalFileAction">
			<result>/jsp/main/simple/showPeriodicalFileDetail.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
	</package>
	<package name="bpim-searchPricePackageFile" namespace="/main" extends="bpim-default">
		<action name="searchPricePackageFile" method="searchPricePackageFile" class="pricePackageFileAction">
			<result>/jsp/main/simple/listPricePackageFile.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
	</package>
	<package name="bpim-downloadPricePackageFile" namespace="/main" extends="bpim-default">
		<action name="downloadPricePackageFile" method="downloadPricePackageFile" class="pricePackageFileAction">
			<result>/jsp/main/simple/downloadPricePackageFile.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
	</package>
	<package name="bpim-main" namespace="/main" extends="bpim-default">
		<action name="enterpriseMain" >
			<result>/jsp/main/enterpriseMain.jsp</result>
		</action>
		<action name="get" method="get" class="userInfoMainAction">
			<result>/jsp/main/user/updateUserInfo.jsp</result>
		</action>
		<action name="listPrice" method="newUser" class="userInfoAction">
			<result>/jsp/main/user/listPrice.jsp</result>
		</action>
		<action name="listRecharge" method="listRecharge" class="enterpriseInfoAction">
			<result>/jsp/main/user/listRecharge.jsp</result>
		</action>
		<action name="main" method="listNewsAnnounces" class="newAnnounceAction">
			<result>/jsp/main/main.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
		<action name="showNews" method="getDetailNewsAnnounces" class="newAnnounceAction">
			<result>/jsp/main/user/showNews.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
		<action name="userInfoManage" >
			<result>/jsp/main/user/updateUserInfo.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
		<action name="updateUserInfo" method="updateUserInfo" class="userInfoMainAction" >
			<result type="redirect-action">main.do</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
	</package>	
	<package name="bpim-addUserAdvice" namespace="/main" extends="bpim-default">
		<action name="addUserAdvice" method="addUserAdvice" class="userAdviceAction">
			<result>/jsp/main/user/userAdvice.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
	</package>
	<package name="bpim-preAddUserAdvice" namespace="/main" extends="bpim-default">
		<action name="preAddUserAdvic">
			<result>/jsp/main/user/userAdvice.jsp</result>
			<result name="nosession" type="redirect">/index.jsp</result>
			<interceptor-ref name="user"/>
		</action>
	</package>
	
</struts>