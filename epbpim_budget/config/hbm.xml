<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.ryxx.bpim.user.entity">
	<class name="AdminRole" table="ADMIN_ROLE">
		<id name="id" column="ROLE_ID" type="java.lang.Long">
			<generator class="identity"></generator>
		</id>
		<property name="name" column="ROLE_NAME" type="java.lang.String"></property>
		<property name="roleCount" column="ROLE_COUNT" type="java.lang.Long"></property>
		<property name="remark" column="ROLE_REMARK" type="java.lang.Integer"></property>
	</class>
	<class name="UserGroup" table="USER_GROUP">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="identity"></generator>
		</id>
		<property name="groupName" column="GROUP_NAME" type="java.lang.String"></property>
		<property name="groupLeaderId" column="GROUP_LEADER_ID" type="java.lang.Long"></property>
		<property name="groupDescription" column="GROUP_DESCRIPTION" type="java.lang.String"></property>
	</class>
	<class name="UserInfo" table="USER_INFO">
		<id name="id" column="ID" type="java.lang.Long">
			<generator class="identity"></generator>
		</id>
		<property name="groupName" column="USER_NAME" type="java.lang.String"></property>
		<property name="groupLeaderId" column="USER_PASSWORD" type="java.lang.String"></property>
		<property name="groupDescription" column="REGISTER_DATE" type="java.sql.Timestamp"></property>
		<property name="groupDescription" column="EXPIRE_DATE" type="java.sql.Timestamp"></property>
		<property name="groupDescription" column="EMAIL" type="java.lang.String"></property>
		<property name="groupDescription" column="COMPANY" type="java.lang.String"></property>
		<property name="groupDescription" column="PHONE" type="java.lang.String"></property>
		<property name="groupDescription" column="LAST_PAY_DATE" type="java.sql.Timestamp"></property>
		<property name="groupDescription" column="QUESTION" type="java.lang.String"></property>
		<property name="groupDescription" column="ANSWER" type="java.lang.String"></property>
	</class>
</hibernate-mapping>