		<property name="projectName" column="PROJECT_NAME" type="java.lang.String"></property>
		<property name="projectNameFeature" column="PROJECT_NAME_FEATURE" type="java.lang.String"></property>
		<property name="unit" column="UNIT" type="java.lang.String"></property>
		<property name="amount" column="AMOUNT" type="java.lang.Double"></property>
		<property name="detail" column="DETAIL" type="java.lang.String"></property>
		<property name="comments" column="COMMENTS" type="java.lang.String"></property>
