		<property name="content" column="NEWS_CONTENT" type="java.lang.String"></property>
		<property name="addTime" column="ADD_TIME" type="java.sql.Timestamp"></property>
	</class>
	<class name="UserAdvice" table="USER_ADVINCE">
		<id name="id" column="ID" type="java.lang.Integer">
			<generator class="identity"></generator>
		</id>
		<property name="userID" column="USER_ID" type="java.lang.String"></property>
		<property name="adviceContent" column="ADVICE_CONTENT" type="java.lang.String"></property>
		<property name="adviceDate" column="ADVICE_DATE" type="java.sql.Timestamp"></property>
	</class>
</hibernate-mapping>